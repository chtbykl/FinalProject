    "Issuer": "cihat@cihat.com", // bunu uygulayan
    "AccessTokenExpiration": 10, // bizim verdiğimiz tokenın geçerlilik süresinin belirtir dk cinsinden
    "SecurityKey": "mysupersecretkeymysupersecretkey" //bu tokenı oluştururken asp.netin kullanacağı anahtarın ismi..
  }
